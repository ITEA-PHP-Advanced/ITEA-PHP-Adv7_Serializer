{
    "name": "greeflas/serializer",
    "description": "This is simple library for objects serialization to needed format.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Kuprienko",
            "email": "vldmr.kuprienko@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Greeflas\\Serializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greeflas\\Serializer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs-check": "php-cs-fixer fix --diff --ansi --dry-run",
        "cs-fix": "php-cs-fixer fix --diff --ansi"
    }
}
